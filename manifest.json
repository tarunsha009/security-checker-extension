{
    "manifest_version": 3,
    "name": "Security Checker",
    "version": "1.0",
    "description": "Check the security status of websites, including SSL certificates and vulnerabilities.",
    "permissions": [
      "tabs",
      "webRequest",
      "storage",
      "notifications" 
    ],
    "host_permissions": [
      "<all_urls>" 
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  